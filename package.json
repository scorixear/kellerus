    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "^0.11.0",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.0.3",
    "discord.ts-architecture": "^1.0.10",
    "dotenv": "^16.0.1",
    "ffmpeg-static": "^5.0.2",
    "mariadb": "^3.0.1",
